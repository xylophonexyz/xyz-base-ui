    "reflect-metadata": "^0.1.10",
    "request": "^2.81.0",
    "rxjs": "6.1.0",
    "rxjs-compat": "^6.0.0-rc.0",
    "simple-oauth2": "^1.1.0",
    "speakingurl": "^14.0.1",
    "tinycolor2": "^1.4.1",
    "url": "^0.11.0",
    "zone.js": "0.8.26"
