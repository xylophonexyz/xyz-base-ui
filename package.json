{
  "name": "xyz-base-ui",
  "version": "0.1.0",
  "license": "LICENSE",
  "scripts": {
    "ng": "ng",
    "dev": "npm run compile:server:dev && concurrently \"ng serve --aot --proxy-config proxy.conf.json\" \"npm run run:server:dev\" -k",
    "test": "ng test --sourcemaps=false",
    "test:singleRun": "ng test --singleRun",
    "test:coverage": "ng test --code-coverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build",
    "build:server": "npm run compile:server && ng build --prod --aot --build-optimizer=false --app 1",
    "compile:server": "tsc src/server.ts --lib es6,dom --target es5",
    "compile:server:dev": "tsc src/server.dev.ts --lib es6,dom --target es5",
    "build:prod": "ng build --prod --aot --build-optimizer=false --app 0",
    "build:full": "npm run build:server && npm run build:prod",
    "start": "NODE_ENV=production npm run run:prod",
    "run:prod": "node dist/server/server.js",
    "run:server:dev": "node --inspect src/server.dev.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@types/autolinker": "^0.24.28",
    "@types/express": "^4.0.35",
    "@types/memcached": "^2.2.3",
    "@types/quill": "^1.3.2",
    "@types/request": "^2.0.3",
    "@types/tinycolor2": "^1.1.1",
    "angular2-moment": "^1.3.3",
    "body-parser": "^1.17.1",
    "bulma": "^0.5.0",
    "compression": "^1.7.0",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "feather-icons": "^3.2.2",
    "font-awesome": "^4.7.0",
    "helmet": "^3.8.1",
    "http-proxy": "^1.16.2",
    "localforage": "^1.5.0",
    "lodash": "^4.17.4",
    "memcached": "^2.2.2",
    "quill": "^1.3.1",
    "reflect-metadata": "^0.1.10",
    "request": "^2.81.0",
    "rxjs": "^5.5.2",
    "simple-oauth2": "^1.1.0",
    "speakingurl": "^14.0.1",
    "tinycolor2": "^1.4.1",
    "url": "^0.11.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.4",
    "@angular/compiler-cli": "^5.0.0",
    "@types/jasmine": "^2.5.47",
    "@types/lodash": "^4.14.64",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.5.0",
    "jasmine-core": "~2.6.1",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.3",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "2.5.3"
  }
}
